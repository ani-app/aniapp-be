{
    "collectCoverage" : true,
    "coveragePathIgnorePatterns" : [
        "/node_modules/"
    ],
    "moduleFileExtensions": [
        "js",
        "mjs"
    ],
    "transform": {
        "^.+\\.js$": "babel-jest",
        "'.+\\.mjs$": "babel-jest"
    },
    "testRegex": "((\\.|/*.)(spec|test))\\.js?$",
    "verbose": true
}